{
  "name": "funkster-http-headers-content",
  "version": "0.6.1",
  "description": "Funkster is a compositional server library. This package contains combinators to parse and set the HTTP Content-* headers.",
  "license": "Unlicense",
  "repository": {
    "type": "git",
    "url": "git@github.com:Bomret/funkster-http-headers-content.git"
  },
  "author": "Stefan Reichel <dev@bomret.com>",
  "keywords": [
    "node",
    "server",
    "funkster",
    "http",
    "content",
    "header"
  ],
  "files": [
    "index.js",
    "lib",
    "package.json",
    "README.md",
    "CHANGELOG.md"
  ],
  "main": "index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
    "build": "npm run clean && npm run lint && echo Using TypeScript && tsc --version && tsc --pretty",
    "test": "npm run build && mocha --compilers ts:ts-node/register --recursive test/**/*-spec.ts",
    "watch": "npm run build -- --watch",
    "watch:test": "npm run test -- --watch"
  },
  "dependencies": {
    "content-disposition": "^0.5.1",
    "content-type": "^1.0.2",
    "funkster-http": "^0.6.0"
  },
  "devDependencies": {
    "@types/content-disposition": "^0.5.0",
    "@types/content-type": "^1.0.33",
    "@types/mocha": "2.2.33",
    "@types/node": "6.0.31",
    "@types/supertest": "^2.0.0",
    "funkster-core": "^0.6.1",
    "mocha": "^3.0.1",
    "rimraf": "^2.5.4",
    "supertest": "^2.0.1",
    "ts-node": "^1.2.2",
    "tslint": "4.0.2",
    "typescript": "^2.0.10"
  },
  "engines": {
    "node": ">=4.6.1"
  }
}
